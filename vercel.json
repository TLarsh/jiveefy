
  {
    "builds": [
      {
        "src": "jiviefy/wsgi.py",
        "use": "@vercel/static-build",
        "config": {
          "maxLambdaSize": "5mb"
        }
      },
      {
        "src": "build_files.sh",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "staticfiles_build"
        }
      }
    ],
    "routes": [
      {
        "src": "/static/(.*)",
        "dest": "static/$1"
      },
      {
        "src": "/(.*)",
        "dest": "jiviefy/wsgi.py/$1"
      }
    ],
    "env": {
      "MY_VARIABLE": "my-value"
    }
  }
  